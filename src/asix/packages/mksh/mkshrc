export PATH="$PATH:/Software/mksh/bin"
export PATH="$PATH:/Software/busybox/bin"
/Software/busybox/bin/busybox --install -s /Software/busybox/bin

# the next few lines belong to an init script
mkdir /dev
mkdir /proc
mkdir /sys
mount -n -t devtmpfs none /dev
mount -t proc /proc /proc
mount -t sysfs none /sys
#echo 0 > /proc/sys/kernel/printk
mknod /dev/null c 1 3
mknod /dev/tty c 5 0
